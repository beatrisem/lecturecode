
public class TaskVar {
	
	/**
	 * 125 + 24 = 149
125 - 24 = 101
125 x 24 = 3000
125 / 24 = 5
125 mod 24 = 5
	 * 
	 * @param args
	 */

	public static void main(String[] args) {
		int numAdd;
		numAdd = 125 + 24;
	
		System.out.println(numAdd + " = 125 + 24");
		
		int numTake;
		numTake = 125 - 24;
	
		System.out.println(numTake + " = 125 - 24");
		
		int numMult;
		numMult = 125 * 24;
	
		System.out.println(numMult + " = 125 * 24");
		
		int numDivide;
		numDivide = 125 + 24;
	
		System.out.println(numDivide + " = 125 / 24");
		
		int numMod;
		numMod = 125 + 24;
	
		System.out.println(numMod + " = 125 % 24");
		
		
		
		

	}

}
