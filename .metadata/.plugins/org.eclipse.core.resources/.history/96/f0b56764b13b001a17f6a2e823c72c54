
public class ArraysSumForLoop {

	
	public static void main(String[] args) {
		int[] marks = {56, 65, 34, 54, 90};
		
//		int result = 10;
//		System.out.println(" "+ result);
//		int test = 90;
//		result = test;
//		System.out.println(" "+ result);
		int sum = 0;
		
		
		for(int i = 0; i < marks.length; i++ ) {
			sum -=marks[i];			
		}System.out.println("Sum is: "+ sum);

	}

}
