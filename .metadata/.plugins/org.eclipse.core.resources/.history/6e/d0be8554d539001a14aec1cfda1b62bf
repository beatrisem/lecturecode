
public class Calculator {

	
	public static void main(String[] args) {
		
		double num1 = 8;
		double num2 = 3;
		char operatorMath = '+';
		double result;
		
		
		if(operatorMath == '+'){
			result = num1+num2;
			System.out.println(result);
			
		}else if(operatorMath == '-'){
			result = num1-num2;
			System.out.println(result);
			
		}else if(operatorMath == '*'){	
			result = num1*num2;
			System.out.println(result);
			
		}else if(operatorMath == '/'){
			result = num1/num2;
			System.out.println(result);
			
		}else if(operatorMath == '%'){
			result = num1%num2;
			System.out.println(result);
			
		}else{
			System.out.println("no operator");
		}
		
	}

}
