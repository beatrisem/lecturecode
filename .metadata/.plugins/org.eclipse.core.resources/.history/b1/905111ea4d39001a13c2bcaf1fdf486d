
public class PrimType {

	
	public static void main(String[] args) {
		// Numeric data type primitive
		int integer = 2;
		long longInteger = 2345612345678L;
		short shortInteger = 3245;
		byte byteInteger = 121;
		// floating-point type real numbers
		double doubleFloatNumber = 0.5;
		float floatNumber = 0.93673857932f;
		// characters
		char charSymbol = 'a';
		char charSymbolb = 102;
		char charSymbole = 105;
		char charSymbola = 65;
		System.out.println("Char:" + charSymbolNum);
	}

}
