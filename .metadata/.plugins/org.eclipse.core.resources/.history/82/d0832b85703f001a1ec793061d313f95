package lv.beatrise.repeatarrays;

import java.util.Scanner;

public class ScannerAndArrays {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		int[] arr = new int[3];
		arr[0] = 10;
		arr[1] = 7;
		arr[2] = 3;
		
		
		
		System.out.println(arr[0]+" "+arr[1]+" "+arr[2]);
		

	}

}
