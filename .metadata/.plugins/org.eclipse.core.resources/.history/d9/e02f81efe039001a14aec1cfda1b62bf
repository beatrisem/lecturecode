
public class TestIfElseAndBlocks {


	public static void main(String[] args) {
		System.out.println("******");
		System.out.println("******");
		
		boolean test = true;
		
		if(test){
			int number = 589;
			System.out.println("true");
			System.out.println(number);
		}else{
			System.out.println("false");
		}
		
		System.out.println("after if else block");
		
		boolean test2 = false;
		
		if(test2){
			System.out.println("true");
		}else{
			System.out.println("false");
		}
		System.out.println("after if else block");
		
	}

}
