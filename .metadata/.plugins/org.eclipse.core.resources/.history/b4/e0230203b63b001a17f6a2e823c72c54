
public class ForEach {

	
	public static void main(String[] args) {
		int[] marks = {56, 65, 34, 54, 90};
		//foreach loop for(type var : arrays)
		for(int mark : marks) {
			
			System.out.println(mark);
		}
		
		for(int i = 0; i < marks.length-1; i++ ){
			
			System.out.println(marks[i]);
			
		}

	}

}
