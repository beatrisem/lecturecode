package lv.beatrise.layer;

public class FootballPlayer {
//define values
	String name;
	String position;
	int shirtNumber;
	boolean build;
	long height;
	int weight;
	
//set getters so that would return value	
	public String getName(){
		return name;
	}
	public 	String getPosition(){
		return position;
	}
	
	public int getshirtNumber(){
		return shirtNumber;
	}
	public boolean getBuild(){
		return build;
	}
	public long getHeight(){
		return height;
	}
	public int getWeight(){
		return weight;
	}
}
