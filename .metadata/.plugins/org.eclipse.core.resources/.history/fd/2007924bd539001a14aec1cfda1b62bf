
public class Calculator {

	
	public static void main(String[] args) {
		
		double num1 = 8;
		double num2 = 3;
		char operatorMath = '+';
		
		if(operatorMath == '+'){
			System.out.println(num1+num2);
		}else if(operatorMath == '-'){
			System.out.println(num1-num2);
		}else if(operatorMath == '*'){
			System.out.println(num1*num2);
		}else if(operatorMath == '/'){
			System.out.println(num1/num2);
		}else if(operatorMath == '%'){
			System.out.println(num1%num2);
		}else{
			System.out.println("no operator");
		}
		
	}

}
